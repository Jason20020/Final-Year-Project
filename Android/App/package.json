{
  "name": "app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "generate-report": "junit-viewer --results=test-results --save=test-results/report.html",
    "test:e2e:android": "APPIUM_PLATFORM=android mocha ./e2e/*.spec.js",
    "test:e2e:ios": "APPIUM_PLATFORM=ios mocha ./e2e/*.spec.js"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-results",
          "outputName": "junit.xml"
        }
      ]
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js",
      "./enzyme.setup.js"
    ]
  },
  "dependencies": {
    "@expo/webpack-config": "^0.17.2",
    "@react-native-community/datetimepicker": "6.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native/normalize-color": "^2.1.0",
    "@react-navigation/bottom-tabs": "^6.5.5",
    "@react-navigation/drawer": "^6.6.0",
    "@react-navigation/native": "^6.1.4",
    "@react-navigation/native-stack": "^6.9.10",
    "@react-navigation/stack": "^6.3.13",
    "@testing-library/react-native": "^12.0.0",
    "expo": "~47.0.12",
    "expo-camera": "~13.1.0",
    "expo-file-system": "^15.1.1",
    "expo-image-picker": "^14.0.2",
    "expo-media-library": "^15.0.0",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "firebase": "8.2.3",
    "jest": "^26.6.3",
    "jest-expo": "^48.0.2",
    "lottie-react-native": "^5.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-date-picker": "^4.2.9",
    "react-native-dropdown-select-list": "^2.0.4",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-image-picker": "^5.0.1",
    "react-native-paper": "^5.1.4",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-web": "~0.18.9",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@cfaester/enzyme-adapter-react-18": "^0.7.0",
    "detox": "^20.6.0",
    "enzyme": "^3.11.0",
    "jest-junit": "^15.0.0",
    "junit-viewer": "^4.11.1",
    "mocha": "^10.2.0",
    "react-test-renderer": "^18.2.0",
    "wd": "^1.14.0"
  },
  "private": true
}
